<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")


    <link href="~/Content/Select2/select2.min.css" rel="stylesheet" />
    <link href="~/Content/Chosen/chosen.min.css" rel="stylesheet" />
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>

            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav" style="font-size:25px;">
                    <li>@Html.ActionLink("Öğrenciler", "Index", "Ogrencis")</li>
                    <li>@Html.ActionLink("Dersler", "Index", "Dersler")</li>
                    <li>@Html.ActionLink("Organizasyon", "Index", "Organizasyons")</li>
                    <li>@Html.ActionLink("Ögrenci-Ders", "Index", "OgrenciDers")</li>



                </ul>
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
       

        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>

    @*@Scripts.Render("~/bundles/jquery")*@
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)

    <script src="~/Scripts/chosen2.jquery.min.js"></script>
    <script src="~/Content/Select2/select2.min.js"></script>

</body>
</html>
